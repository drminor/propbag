<Window x:Class="MVVMApplication.FrontWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVMApplication"
        mc:Ignorable="d"
        Title="FrontWindow" Height="320" Width="320" MinHeight="200" MinWidth="150">
    <Border BorderThickness="1" BorderBrush="Black" Margin="1,1,1,1" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Button Name="btnPlain"  Grid.Row="0" Content="Plain"
                HorizontalAlignment="Center" Margin="0,25,0,0" VerticalAlignment="Top" Width="160" Height="45" Click="Plain_Click" />

        <Button Name="btnPropBagProxyEmit" Grid.Row="1" Content="PropBag (Emit Proxy)"
                HorizontalAlignment="Center" Margin="0,25,0,0" VerticalAlignment="Top" Width="160" Height="45" Click="PropBagProxyEmit_Click"/>

        <Button Name="btnPropBagExtraMembers" Grid.Row="2" Content="PropBag (Extra Members)"
                HorizontalAlignment="Center" Margin="0,25,0,0" VerticalAlignment="Top" Width="160" Height="45" Click="PropBagExtraMembers_Click"/>

    </Grid>
    </Border>
</Window>
